/* pages/category/index.wxss */
page {
  height: 100%;
}
.cates {
  height: 100%;
}
.cates .cateContainer {
  height: calc( 100vh - 90rpx );
  display: flex;
}
.cates .cateContainer .left_menu {
  flex: 2;
}
.cates .cateContainer .left_menu .menu_item {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
}
.cates .cateContainer .left_menu .active {
  color: var(--themeColor);
  border-left: 5rpx solid currentColor;
}
.cates .cateContainer .right_menu {
  flex: 5;
}
.cates .cateContainer .right_menu .goods_group .goods_title {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 80rpx;
}
.cates .cateContainer .right_menu .goods_group .goods_title .seperate {
  color: #ccc;
  padding: 0 10rpx;
}
.cates .cateContainer .right_menu .goods_group .goods_list {
  display: flex;
  flex-wrap: wrap;
}
.cates .cateContainer .right_menu .goods_group .goods_list navigator {
  width: 33.33%;
  text-align: center;
}
.cates .cateContainer .right_menu .goods_group .goods_list image {
  width: 50%;
}
/*# sourceMappingURL=./index.wxss.map */